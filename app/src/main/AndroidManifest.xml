<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lyf.androidlyfcode">
    <!-- 访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".Applications"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewStudy" />
        <activity android:name=".BaseViewActivity" />
        <activity android:name=".view.rectangle.RectangleActivity" />
        <activity android:name=".BlendViewActivity" />
        <activity android:name=".fragment.FragmentActivity"/>
        <activity android:name=".lvrv.RVLVMainActivity"></activity>
        <activity android:name=".Measure.MeasureActivity"></activity>
        <activity android:name=".Behavior.CusBehaviorActivity"></activity>
        <activity
            android:name=".sonic.SonicTestActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".scroll.ScrollActivity"/>
        <activity android:name=".view.line.CusLineActivity"/>
    </application>
</manifest>